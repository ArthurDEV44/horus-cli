{
  "name": "@vibe-kit/horus-cli",
  "version": "0.0.33",
  "description": "An open-source AI agent that brings the power of Horus directly into your terminal.",
  "type": "module",
  "packageManager": "pnpm@10.23.0",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "horus": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:bun": "bun run tsc",
    "dev": "bun run src/index.ts",
    "dev:node": "tsx src/index.ts",
    "start": "node dist/index.js",
    "start:bun": "bun run dist/index.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "install:bun": "bun install"
  },
  "keywords": [
    "cli",
    "agent",
    "text-editor",
    "horus",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "axios": "^1.13.2",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.2",
    "ink": "^6.5.1",
    "marked": "^17.0.1",
    "marked-terminal": "^7.3.0",
    "openai": "^6.9.1",
    "react": "^19.2.0",
    "ripgrep-node": "^1.0.0",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true
}
