{
  "name": "@vibe-kit/horus-cli",
  "version": "0.0.33",
  "description": "An open-source AI agent that brings the power of Horus directly into your terminal.",
  "type": "module",
  "packageManager": "bun@1.3.5",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "horus": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:bun": "bun run tsc",
    "dev": "bun run src/index.ts",
    "dev:node": "tsx src/index.ts",
    "start": "node dist/index.js",
    "start:bun": "bun run dist/index.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "install:bun": "bun install"
  },
  "keywords": [
    "cli",
    "agent",
    "text-editor",
    "horus",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@types/turndown": "^5.0.6",
    "axios": "^1.13.2",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.3",
    "ink": "^6.6.0",
    "lru-cache": "^11.2.4",
    "marked": "^17.0.1",
    "marked-terminal": "^7.3.0",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "ripgrep-node": "^1.0.0",
    "tiktoken": "^1.0.22",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chokidar": "^2.1.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true
}
